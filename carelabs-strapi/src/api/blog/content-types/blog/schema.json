{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "link": {
      "type": "string"
    },
    "slug": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "pubDate": {
      "type": "date"
    },
    "content": {
      "type": "richtext"
    },
    "featureImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "seo": {
      "type": "component",
      "component": "shared.seo",
      "repeatable": false
    },
    "blog_category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::blog-category.blog-category"
    },
    "category": {
      "type": "string"
    }
  }
}
